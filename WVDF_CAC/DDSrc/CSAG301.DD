Use  Windows           // Basic Definitions
Use  DataDict.pkg          // DataDictionary Class Definition
Use  DDvalTbl          // Validation Table Class Definitions

Open Csag301


Register_Object Csag301_Tipo_VT
Register_Object Csag301_Localizacao_VT
Register_Object Csag301_Infraestrutura_VT
Register_Object Csag301_Status_VT
Register_Object Csag301_Classe_VT
Register_Object Csag301_Tipomenu_VT

Object Csag301_Tipo_VT  is a DescriptionValidationTable
    Set Validate_State    To FALSE

    Procedure Fill_List
        Forward Send Fill_List
        Send Add_Table_Value  "1"  "Programa"
        Send Add_Table_Value  "2"  "Menu"
        Send Add_Table_Value  "3"  "Titulo"
        Send Add_Table_Value  "4"  "SubTitulo"
    End_Procedure    // Fill_List
End_Object   // Csag301_Tipo_VT

Object Csag301_Localizacao_VT  is a DescriptionValidationTable

    Procedure Fill_List
        Forward Send Fill_List
        Send Add_Table_Value "F" "ABAS"
        Send Add_Table_Value "W" "WINDOW"
        Send Add_Table_Value "S" "SITE"
    End_Procedure    // Fill_List
End_Object   // Csag301_Localizacao_VT

