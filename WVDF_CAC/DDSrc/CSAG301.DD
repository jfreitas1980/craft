Class Csag301_DataDictionary  is a DataDictionary

    // Define_Fields:
    // This procedure is used to set up all data-dictionary rules.

    Procedure Construct_Object
        Forward Send Construct_Object


        Set Main_File To Csag301.File_Number

        Set Foreign_Field_Option DD_KEYFIELD DD_AUTOFIND to True
        Set Foreign_Field_Option DD_KEYFIELD DD_NOPUT to True
        Set Foreign_Field_Option DD_KEYFIELD DD_FINDREQ to True
        Set Foreign_Field_Option DD_INDEXFIELD DD_NOPUT to True
        Set Foreign_Field_Option DD_INDEXFIELD DD_FINDREQ to True
        Set Foreign_Field_Option DD_DEFAULT DD_DISPLAYONLY to True

        // Field-based properties.......................

        Set Field_Label_Long Field Csag301.Menuclasse To "Classe"
        Set Field_Option Field Csag301.Menuclasse DD_RETAINALL to True

        Set Field_Label_Long Field Csag301.Menunumero To "Numero"
        Set Field_Option Field Csag301.Menunumero DD_RETAINALL to True

        Set Field_Label_Long Field Csag301.Titulo To "Titulo"

        Set Field_Label_Long Field Csag301.Tipo To "Tipo"
        Set Field_Option Field Csag301.Tipo DD_RETAINALL to True
        Set Field_Value_Table Field Csag301.Tipo to Csag301_Tipo_VT

        Set Field_Label_Long Field Csag301.Programa To "Programa"
        Set Field_Option Field Csag301.Programa DD_RETAINALL to True

        Set Field_Label_Long Field Csag301.Localizacao To "Localizacao"
        Set Field_Option Field Csag301.Localizacao DD_RETAINALL to True
        Set Field_Value_Table Field Csag301.Localizacao to Csag301_Localizacao_VT

        Set Field_Label_Long Field Csag301.Mcchamada To "Menu De Chamada"
        Set Field_Option Field Csag301.Mcchamada DD_RETAINALL to True

        Set Field_Label_Long Field Csag301.Programacodigo To "Codigo Do Programa"
        Set Field_Option Field CSAG301.PROGRAMACODIGO DD_CAPSLOCK to True

        Set Field_Label_Long Field Csag301.Infraestrutura To "Infra Estrutura "
        Set Field_Option Field Csag301.Infraestrutura DD_RETAINALL to True
        Set Field_Value_Table Field Csag301.Infraestrutura to Csag301_Infraestrutura_VT

        Set Field_Value_Table Field Csag301.Status to Csag301_Status_VT
        Set Field_Label_Long Field CSAG301.STATUS to "STATUS"

        Set Field_Option Field Csag301.Classe DD_RETAINALL to True
        Set Field_Value_Table Field Csag301.Classe to Csag301_Classe_VT
        Set Field_Label_Long Field CSAG301.CLASSE to "CLASSE"

        Set Field_Label_Long Field Csag301.Imagem_Fisico To "Imagem Caminho Fisico"

        Set Field_Label_Long Field Csag301.Imagem_Logico To "Imagem Caminho Logico"

        Set Field_Label_Long Field Csag301.Tipomenu To "Tipo de Menu"
        Set Field_Option Field Csag301.Tipomenu DD_RETAINALL to True
        Set Field_Value_Table Field Csag301.Tipomenu to Csag301_Tipomenu_VT

        Set Field_Label_Long Field CSAG301.ICONEB00TSP to "Icone Bootstrap"

    End_Procedure    // Construct_Object

    // Field_Defaults:
    // This procedure is used to establish default field values.

    Procedure Field_Defaults
        Forward Send Field_Defaults
        Set Field_Changed_Value   Field Csag301.Menuclasse To 1
        Set Field_Changed_Value   Field Csag301.Infraestrutura To "2"
    End_Procedure    // Field_Defaults

End_Class    // Csag301_DataDictionary

